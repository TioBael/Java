import java.util.Scanner;

public class IR {
	public static void main(String[] args) {
		Scanner t = new Scanner(System.in);
		double salarioBruto, salarioLiquido;

		System.out.printf("\nDigite o seu salário: ");
		salarioBruto = t.nextDouble();

		if (salarioBruto <= 1903.98)
			salarioLiquido = salarioBruto;
		else if (salarioBruto <= 2826.65)
			salarioLiquido = salarioBruto * 0.075;
		else if (salarioBruto <= 3751.05)
			salarioLiquido = salarioBruto * 0.15;
		else if (salarioBruto <= 4664.68)
			salarioLiquido = salarioBruto * 0.225;
		else
			salarioLiquido = salarioBruto * 0.275;
		
		System.out.printf("\nSalario Bruto: %.2f\nSalario Liquido: %.2f", salarioBruto, salarioLiquido);
		t.close();
	}
}
